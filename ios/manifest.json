{
  "manifest_version": 2,

  "name": "iOS Developer Library Search",
  "description": "Adds an “id” omnibox command for the iOS Developer Library.",
  "version": "0.1.1",

  "permissions": [
    "tabs",
    "https://developer.apple.com/library/ios/*/"
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  },
  "omnibox": {
    "keyword": "id"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "configuration.js",
      "omnibox.js"
    ]
  }
}
